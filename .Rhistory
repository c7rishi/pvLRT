#' @export
extract_significant_pairs <- function(object, significance_level = 0.05, ...) {
if (!is.pvlrt(object)) {
stop("object must be a 'pvlrt' object.")
}
p.value <- NULL
out <- summary(object)[p.value < significance_level]
out
}
